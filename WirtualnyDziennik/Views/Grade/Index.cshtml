@model IList<Object[]> 
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("UCZEN")
        </th>
        <th>
            @Html.DisplayName("OCENA")
        </th>
        <th>
            @Html.DisplayName("PRZEDMIOT")
        </th>
        <th>
            @Html.DisplayName("DATA")
        </th>
    </tr>
    @{

        if (Model != null)
        {

            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item[0])
                    </td>
                    <td>
                        @Html.DisplayFor(model =>item[1])
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[2])
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[3])
                    </td>
                </tr>
            }

        }
    }
</table>




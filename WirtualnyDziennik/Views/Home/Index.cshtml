@model IEnumerable<WirtualnyDziennik.Models.Tresc>

@{
    ViewBag.Title = "Home Page";
    int i = 0;
}
@foreach (var item in Model)
{
    i++;
    if (i > 5) { break; }
<div class="col">
    <div class="col-md-12">
        <h2>@item.tytul</h2>
        <p>
            @item.tresc
        </p>
       <!-- <p><a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkId=301865">Learn more &raquo;</a></p>-->
    </div>
  
</div>
}
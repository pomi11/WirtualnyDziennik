@model IList<Object[]>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("IMIE")
        </th>
        <th>
            @Html.DisplayName("NAZWISKO")
        </th>
    </tr>
    @{
        if (Model != null)
        {

            foreach (var item in Model)
            {
    <tr>
        <td>
           @Html.DisplayFor(model => item[1])
        </td>
        <td>
           @Html.DisplayFor(model => item[2])
        </td>
        
        if (Konto.Typ == "NAUCZYCIEL")
            {
        <td>
            <button onclick="location.href='@Url.Action("Index", "DziennikUcznia", new { iducznia = item[0],planlekcjiid=ViewData["planlekcjiid"] } )'" type="button" class="btn btn-primary btn-xs"><span class="glyphicon glyphicon-edit" aria-hidden="true"></span> Edit </button>
        </td>
    </tr>
            }

        }
    }
    }

</table>
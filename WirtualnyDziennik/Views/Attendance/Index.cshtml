@model IList<Object[]> 
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("UCZEN")
        </th>
        <th>
            @Html.DisplayName("OBECNOSC")
        </th>
        <th>
            @Html.DisplayName("PRZEDMIOT")
        </th>
        <th>
            @Html.DisplayName("DATA")
        </th>
    </tr>
    @{

        if (Model != null)
        {

            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item[0])
                    </td>
                    <td>
                        
                      
                        @switch(item[1].ToString())
                        {
                        case "0":
                        @Html.DisplayName("OBECNY");
                            break;
                        case "1":
                        @Html.DisplayName("SPÓŹNIENIE");
                        break;
                        case "2":
                        @Html.DisplayName("NIEOBECNY");
                        break;
                        }
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[2])
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[3])
                    </td>
                </tr>
            }

        }
    }
</table>




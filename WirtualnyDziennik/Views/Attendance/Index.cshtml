@model IList<Object[]> 
<table class="table">
    <tr>

        @{ var Konto = this.Session["UserProfile"] as WirtualnyDziennik.Models.UserProfileSessionData;
            if (Konto.Typ == "RODZIC")
            {
                <th>
                    @Html.DisplayName("IMIE")
                </th>
                <th>
                    @Html.DisplayName("NAZWISKO")
                </th>
            }

        }
        <th>
            @Html.DisplayName("OBECNOSC")
        </th>
        <th>
            @Html.DisplayName("PRZEDMIOT")
        </th>
        <th>
            @Html.DisplayName("DATA")
        </th>
    </tr>
    @{

        if (Model != null)
        {

            foreach (var item in Model)
            {
                var i = 0;
                <tr>
                    @{
                        if (Konto.Typ == "RODZIC")
                        {
                            <td>
                                @Html.DisplayFor(model => item[i])
                            </td>
                            i++;
                            <td>
                                @Html.DisplayFor(model => item[i])
                            </td>
                            i++;
                        }
                    }
                    <td>
                        
                      
                        @switch(item[i].ToString())
                        {
                        case "0":
                        @Html.DisplayName("OBECNY");
                            break;
                        case "1":
                        @Html.DisplayName("SPÓŹNIENIE");
                        break;
                        case "2":
                        @Html.DisplayName("NIEOBECNY");
                        break;
                        }
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[1+i])
                    </td>
                    <td>
                        @Html.DisplayFor(model => item[2+i])
                    </td>
                </tr>
            }

        }
    }
</table>



